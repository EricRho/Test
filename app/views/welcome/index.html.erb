<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>

<div ng-model='name'>
  <h1>{{ name }}</h1>
  <input type='text' ng-model='name'>
</div>

<div ng-controller="clockCtrl">
  <h2>{{ clock }}</h2>
</div>

<div ng-controller='teamsCtrl'>
  <table class='table table-striped'>
    <tbody>
      <tr ng-repeat='team in teams'>
        <td>{{ team.name }}</td>
      </tr>
    </tbody>
  </table>
</div>
